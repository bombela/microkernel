#pragma once
#ifndef FORWARD_H
#define FORWARD_H

namespace std {
namespace kernel_std {
	
template <typename T>
T&& forward(T&& arg) { return static_cast<T&&>(arg); }

} // namespace kernel_std
} // namespace std

#endif /* FORWARD_H */
